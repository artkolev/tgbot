{
  "name": "artkolev/tgbot",
  "description": "telegram bot",
  "type": "library",
  "license": "GNU",
  "authors": [
    {
      "name": "artkolev",
      "email": "work@artkolev.ru"
    }
  ],
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": false
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-memcached": "*",
    "vlucas/phpdotenv": "^3.5",
    "symfony/http-foundation": "^5.4.46",
    "symfony/console": "^4.3",
    "league/container": "^2.2",
    "nikic/fast-route": "^1.0.0",
    "monolog/monolog": "^1.0.0",
    "doctrine/orm": "^2.6.2",
    "longman/telegram-bot": "^0.80.0",
    "cache/memcached-adapter": "^1.0",
    "cache/simple-cache-bridge": "^1.0",
    "doctrine/cache": "^1.11",
    "doctrine/annotations": "^2.0"
  },
  "require-dev": {
    "symfony/var-dumper": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "TGBot\\": "app/"
    },
    "classmap": [
      "app/Commands/"
    ]
  }
